<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <!-- ex) 02:37:05.310 [main] INFO  com.example.ScrapingDLSite - start Search Box -->
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
    <Console name="Console2" target="SYSTEM_OUT">
      <!-- ex) 02:37:05.310 [main] INFO  com.example.ScrapingDLSite - start Search Box -->
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
    <Console name="Console3" target="SYSTEM_OUT">
      <!-- ex) 02:37:05.310 [main] INFO  com.example.ScrapingDLSite - start Search Box -->
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
    <Console name="Console4" target="SYSTEM_OUT">
      <!-- ex) 02:37:05.310 [main] INFO  com.example.ScrapingDLSite - start Search Box -->
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="com.example.ScrapingDLSite" level="trace" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="com.example.DynamoDbDLSite" level="trace" additivity="false">
      <AppenderRef ref="Console2"/>
    </Logger>
    <Logger name="com.example.DynamoDbDLSite" level="trace" additivity="false">
      <AppenderRef ref="Console3"/>
    </Logger>
    <Logger name="com.example.ControllerDLSite" level="trace" additivity="false">
      <AppenderRef ref="Console4"/>
    </Logger>
    <Root level="trace">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
